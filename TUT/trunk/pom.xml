<!-- This file is for releasing OpenL version with maven-release-plugin -->

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.openl.rules</groupId>
  <artifactId>openl-dev-svn-pom</artifactId>
  <packaging>pom</packaging>
  <version>5.7.6-SNAPSHOT</version>
  <name>OpenL TUT SVN root pom</name>
  <url>http://openl-tablets.sourceforge.net/ws-toc401.shtml</url>

  <scm>
    <connection>scm:svn:https://svn-rix.exigengroup.lv/svn/EPB/EPB/DecisionServices/OpenL/TUT/tags/release-5.5.0</connection>
    <developerConnection>scm:svn:https://svn-rix.exigengroup.lv/svn/EPB/EPB/DecisionServices/OpenL/TUT/tags/release-5.5.0</developerConnection>
  </scm>

  <modules>
    <module>org.openl.rules.tutorials-pom</module>
  </modules>

  <distributionManagement>
    <repository>
      <id>exigen-repo-release</id>
      <name>Exigen Release Repository</name>
      <url>file:////bldrix02.exigengroup.lv/maven-repo/exigen-repo-release</url>
    </repository>
  </distributionManagement>

  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-release-plugin</artifactId>
          <version>2.0-beta-8</version>
        </plugin>
        <plugin>
          <artifactId>maven-deploy-plugin</artifactId>
          <configuration>
            <skip>true</skip>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>

</project>