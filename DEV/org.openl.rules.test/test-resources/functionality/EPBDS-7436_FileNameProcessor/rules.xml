<project>
    <name>FileVersioning</name>
    <modules>
        <module>
            <name>AUTO</name>
            <rules-root path="AUTO-*.xlsx"/>
        </module>
        <module>
            <name>HOME</name>
            <rules-root path="HOME-*.xlsx"/>
        </module>
        <module>
