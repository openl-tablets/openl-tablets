        <module>
            <rules-root path="rules/*.xlsx" />
        </module>
    </modules>

    <properties-file-name-pattern>%state%-%lob%-%effectiveDate%-%startRequestDate%</properties-file-name-pattern>
    <classpath>
        <entry path="lib/*.jar" />
    </classpath>
</project>