<?xml version="1.0" encoding="UTF-8"?>

<facelet-taglib
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibary_2_0.xsd"
        version="2.0">

    <namespace>http://openl-tablets.sf.net/jsf/html</namespace>

    <tag>
        <tag-name>inputVersion</tag-name>
        <source>html/inputVersion.xhtml</source>
    </tag>

    <tag>
        <tag-name>multiselect</tag-name>
        <source>html/multiselect.xhtml</source>
    </tag>

    <tag>
        <tag-name>fileUpload</tag-name>
        <component>
            <component-type>openl.UIFileUpload</component-type>
            <handler-class>org.richfaces.view.facelets.FileUploadHandler</handler-class>
        </component>
    </tag>
</facelet-taglib>
