    "webpack-cli": "6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "scripts": {
    "start": "webpack-cli serve --stats-error-details --config-node-env=development",
    "build": "license-checker-rseidelsohn --onlyAllow \"MIT;Apache-2.0\" --production --excludePrivatePackages && webpack --config-node-env=production",
    "serve": "serve dist -p 3002",
    "clean": "rm -rf dist",
    "lint": "eslint ./src && stylelint '**/*.scss'",
    "lint:fix": "eslint ./src --fix && stylelint '**/*.scss' --fix",
    "test": "exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm run lint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
