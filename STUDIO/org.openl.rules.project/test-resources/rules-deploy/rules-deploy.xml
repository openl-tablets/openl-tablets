<rules-deploy>
  <isProvideRuntimeContext>false</isProvideRuntimeContext>
  <isProvideVariations>true</isProvideVariations>
  <useRuleServiceRuntimeContext>true</useRuleServiceRuntimeContext>
  <serviceName>rulesDeployName</serviceName>
  <serviceClass>java.lang.String</serviceClass>
  <rmiServiceClass>java.lang.String</rmiServiceClass>
  <publishers>
    <publisher>restful</publisher>
  </publishers>
  <loggingStorages>
    <storage>CASSANDRA</storage>
  </loggingStorages>
  <interceptingTemplateClassName>java.lang.String</interceptingTemplateClassName>
  <annotationTemplateClassName>java.lang.String</annotationTemplateClassName>
  <url>someURL</url>
  <rmiName>rmiName</rmiName>
  <version>v1</version>
  <groups>group1,group2</groups>
  <lazy-modules-for-compilation>
    <module name="some1*"/>
    <module name="some2*"/>
  </lazy-modules-for-compilation>
  <configuration>
    <entry>
      <string>rootClassNamesBinding</string>
      <string>com.chartis.premier.rating.result.ChartisCompoundStep,com.chartis.premier.rating.result.ChartisSimpleStep</string>
    </entry>
  </configuration>
</rules-deploy>
